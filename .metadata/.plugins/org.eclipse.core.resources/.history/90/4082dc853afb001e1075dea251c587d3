package springcore.ci;

public class Person {
	private String name;
	private int personId;
	
//	Constructor Injection
	public Person(String name,int personId,Certi certi) {
		this.name=name;
		this.personId=personId;
	}

	@Override
	public String toString() {
		return this.name+" : "+this.personId;
	}
	
	
	
}
